@model OnlineTicariOtomasyon.Models.Current
@{
    ViewBag.Title = "AddCurrent";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h3>Cari Ekle</h3>
<form method="post">
    <label>Cari Adı</label>
    @Html.TextBoxFor(x => x.CurrentName, new { @class = "form-control" })
    <br />
    <label>Cari Soyadı</label>
    @Html.TextBoxFor(x => x.CurrentSurname, new { @class = "form-control" })
    <br />
    <label>Şehir</label>
    @Html.TextBoxFor(x => x.City, new { @class = "form-control" })
    <br />
    <label>Email</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />

    <button class="btn btn-primary">Kaydet</button>
</form>

